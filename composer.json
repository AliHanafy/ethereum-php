{
  "name": "alihanafy/ethereum-php",
  "description": "A library integrating Ethereum with typed PHP.",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "ext-ctype": "*",
    "jackyxie/guzzle-jsonrpc": "^3.2.2",
    "pear/math_biginteger": "^v1.0.3",
    "kornrunner/keccak": "^1.1.0",
    "digitaldonkey/ecverify":"^1.0"
  },
  "scripts": {
    "generate-methods": [
      "php scripts/generate-methods.php"
    ],
    "generate-complex-datatypes": [
      "php scripts/generate-complex-datatypes.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.11"
  },
  "autoload": {
    "psr-4": {
      "Ethereum\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Thorsten Krug",
      "email": "thorsten.krug@consensys.net"
    }
  ]
}
